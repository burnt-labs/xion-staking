{
  "name": "xion-staking",
  "version": "0.2.0",
  "private": true,
  "packageManager": "pnpm@10.28.1",
  "pnpm": {
    "overrides": {
      "react-native": "npm:react-native-noop@^1.0.0"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "react-native"
      ],
      "allowedVersions": {
        "react": "18",
        "react-dom": "18",
        "@types/react": "18",
        "@types/react-dom": "18",
        "@walletconnect/types": "2",
        "@cosmjs/amino": "*",
        "@cosmjs/proto-signing": "*"
      }
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "protobufjs",
      "sharp",
      "tiny-secp256k1",
      "unrs-resolver",
      "workerd"
    ]
  },
  "scripts": {
    "build": "next build",
    "build:quick": "QUICK_BUILD=true next build",
    "changeset": "changeset",
    "changeset:publish": "changeset publish",
    "changeset:version": "changeset version",
    "ci": "pnpm run lint && pnpm run type-check && pnpm run test && pnpm run build:quick",
    "deploy:mainnet": "wrangler deploy --env mainnet",
    "deploy:testnet": "wrangler deploy --env testnet",
    "dev": "PORT=4000 next dev",
    "lint": "pnpm run lint:unused-exports && pnpm run lint:eslint && pnpm run lint:prettier",
    "lint:eslint": "eslint . --ext .ts,.tsx",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:unused-exports": "ts-unused-exports tsconfig.json --excludePathsFromReport='next.config' --excludePathsFromReport='tailwind.config' --excludePathsFromReport='eslint.config' --excludePathsFromReport='.*/app/.*' --excludePathsFromReport='.*\\.next\\.*' --excludePathsFromReport='/server/index.ts'",
    "start": "next start",
    "test": "echo No tests yet",
    "type-check": "tsc --noEmit",
    "update:burnt-deps": "pnpm add -D @burnt-labs/abstraxion@latest @burnt-labs/ui@latest @burnt-labs/constants@latest",
    "workers:dev": "wrangler dev"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@burnt-labs/abstraxion": "^1.0.0-alpha.73",
    "@burnt-labs/constants": "^0.1.0-alpha.20",
    "@burnt-labs/ui": "^1.0.0-alpha.21",
    "@chain-registry/types": "^0.50.56",
    "@cosmjs/cosmwasm-stargate": "^0.32.4",
    "@cosmjs/proto-signing": "^0.32.4",
    "@cosmjs/stargate": "^0.32.4",
    "@cosmjs/tendermint-rpc": "^0.32.4",
    "@cosmos-kit/core": "^2.15.4",
    "@cosmos-kit/cosmos-extension-metamask": "^0.12.7",
    "@cosmos-kit/keplr": "^2.14.6",
    "@cosmos-kit/keplr-extension": "^2.14.5",
    "@cosmos-kit/leap-extension": "^2.14.7",
    "@cosmos-kit/ledger": "^2.13.7",
    "@cosmos-kit/okxwallet-extension": "^2.13.4",
    "@cosmos-kit/react": "^2.21.5",
    "@keplr-wallet/types": "^0.12.76",
    "@mui/base": "^5.0.0-beta.38",
    "@tanstack/react-query": "^4.36.1",
    "axios": "^1.7.7",
    "bignumber.js": "^9.1.2",
    "chain-registry": "^1.69.122",
    "cosmjs-types": "^0.9.0",
    "next": "14.2.23",
    "osmojs": "^16.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.5",
    "react-hook-form": "^7.54.2",
    "react-modal": "^3.16.1",
    "react-toastify": "^10.0.4",
    "react-tooltip": "^5.26.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.11.21",
    "@types/react": "^18.2.60",
    "@types/react-dom": "^18.2.19",
    "@types/react-modal": "^3.16.3",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "autoprefixer": "^10.4.17",
    "esbuild": "^0.25.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "15.1.6",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-perfectionist": "^4.0.0",
    "eslint-plugin-playwright": "^0.22.2",
    "eslint-plugin-tailwindcss": "^3.14.3",
    "http-server": "^14.1.1",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "ts-unused-exports": "^10.0.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.53.1",
    "wrangler": "^4.10.0"
  }
}
